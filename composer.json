{
  "name": "systemseed/falcon",
  "description": "Open sourced repository for charity organizations.",
  "type": "drupal-profile",
  "authors": [
    {
      "name": "SystemSeed",
      "homepage": "https://systemseed.com",
      "role": "Company"
    }
  ],
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "process-timeout": 0,
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "composer drupal:scaffold",
      "@falcon:scaffold"
    ],
    "post-update-cmd": [
      "composer drupal:scaffold",
      "@falcon:scaffold"
    ],
    "falcon:scaffold": [
      "mkdir -p web/profiles/contrib/falcon",
      "@falcon:links"
    ],
    "falcon:links": "cd web/profiles/contrib/falcon;ln -fs ../../../../config .;ln -fs ../../../../modules .;ln -fs ../../../../falcon.* ."
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "source": "https://raw.githubusercontent.com/drupal/drupal/{version}/{path}",
      "initial": {
        "sites/default/default.services.yml": "sites/default/services.yml",
        "sites/default/default.settings.php": "sites/default/settings.php"
      }
    },
    "enable-patching": true,
    "patches": {}
  },
  "require": {
    "composer/installers": "^1.5.0",
    "cweagans/composer-patches": "^1.6.0",
    "drupal-composer/drupal-scaffold": "^2.5.0",
    "drupal/admin_toolbar": "^1.23",
    "drupal/config_filter": "^1.2",
    "drupal/config_ignore": "^2.1",
    "drupal/config_pages": "^2.2",
    "drupal/config_split": "^1.3",
    "drupal/consumer_image_styles": "^2.0@rc",
    "drupal/consumers": "^1.0@beta",
    "drupal/contact_message_rest": "^1.0@alpha",
    "drupal/core": "^8.5.6",
    "drupal/eck": "^1.0@alpha",
    "drupal/entity": "^1.0@beta",
    "drupal/entity_reference_revisions": "^1.5",
    "drupal/field_group": "^3.0@beta",
    "drupal/fieldable_path": "^1.0@rc",
    "drupal/jsonapi": "^1.23",
    "drupal/jsonapi_extras": "^2.5",
    "drupal/mailsystem": "^4.1",
    "drupal/material_admin": "^1.0@alpha",
    "drupal/metatag": "^1.5",
    "drupal/paragraphs": "^1.3",
    "drupal/redirect": "^1.2",
    "drupal/restui": "^1.15",
    "drupal/rest_absolute_urls": "^1.0@beta",
    "drupal/scheduler": "^1.0",
    "drupal/smtp": "^1.0@beta",
    "drupal/token": "^1.3"
  },
  "require-dev": {
    "drupal/coder": "^8.2",
    "drupal/config_distro": "^1.0@alpha",
    "drupal/config_merge": "^1.0@alpha",
    "drupal/config_provider": "^2.0@alpha",
    "drupal/config_snapshot": "^1.0@alpha",
    "drupal/config_sync": "^2.0@alpha",
    "drupal/config_update": "^1.5",
    "drupal/devel": "^1.2",
    "drupal/features": "^3.7",
    "drupal/reroute_email": "^1.0"
  }
}
